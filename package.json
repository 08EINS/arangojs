{
  "name": "arangojs",
  "version": "5.8.0",
  "license": "Apache-2.0",
  "description": "The official ArangoDB JavaScript driver.",
  "homepage": "https://github.com/arangodb/arangojs",
  "bugs": {
    "url": "https://github.com/arangodb/arangojs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arangodb/arangojs.git"
  },
  "author": "ArangoDB GmbH",
  "contributors": [
    {
      "name": "Alan Plum",
      "email": "me@pluma.io"
    }
  ],
  "keywords": [
    "arango",
    "arangodb",
    "aql",
    "nosql",
    "client",
    "driver",
    "api",
    "http",
    "rest"
  ],
  "main": "lib/index.js",
  "browser": {
    "./lib/util/btoa.js": "./lib/util/btoa.web.js",
    "./lib/util/bytelength.js": "./lib/util/bytelength.web.js",
    "./lib/util/joinPath.js": "./lib/util/joinPath.web.js",
    "./lib/util/multipart.js": "./lib/util/multipart.web.js",
    "./lib/util/request.js": "./lib/util/request.web.js"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/util/*.js",
    "lib/util/*.d.ts",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "typings": "arangojs.d.ts",
  "scripts": {
    "test":
      "mocha --growl --reporter spec --require source-map-support/register --require core-js/shim --timeout 5000 lib/test",
    "pretest": "npm run dist",
    "lint": "eslint src/**/*.js test/**/*.js",
    "ci":
      "mocha --reporter spec --require source-map-support/register --require core-js/shim --timeout 5000 lib/test",
    "preci": "npm install",
    "jenkins": "npm run ci -- --timeout 0 --reporter xunit-file",
    "dist": "tsc -p .",
    "prepublish": "npm run lint && npm run dist && node -e 'require(\"./\");'"
  },
  "dependencies": {
    "babel-eslint": "^8.0.3",
    "es6-error": "^4.0.1",
    "http-errors": "^1.6.1",
    "multi-part": "^2.0.0",
    "retry": "^0.10.0",
    "utf8-length": "^0.0.1",
    "xhr": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.58",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-env": "^2.4.1",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "chai": "^4.0.2",
    "core-js": "^2.4.1",
    "eslint": "^4.13.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "3.4.2",
    "prettier": "^1.9.1",
    "source-map-support": "^0.5.0",
    "ts-mocha": "^1.0.3",
    "typescript": "^2.6.2",
    "webpack": "^3.0.0",
    "xunit-file": "1.0.0"
  }
}
